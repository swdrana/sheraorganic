{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true  // Add 'node' to enable Node.js global variables
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "process": "readonly"  // Define 'process' as a global variable
  },
  "plugins": ["react", "react-hooks","next"],
  "rules": {
    "no-unused-vars": "warn"
  }
}
